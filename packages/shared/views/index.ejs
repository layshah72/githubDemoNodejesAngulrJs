   <script>
    var counter = 1;
	function changebg() {
	    if (counter == 1) {
	        jQuery('#carousel1').fadeOut(1500);
	        jQuery('#carousel2').removeClass(".hidebg").addClass(".showbg");
	        jQuery('#line1').slideUp(100);
	        jQuery('#line2').slideUp(100);
	        jQuery("#carousel2").fadeIn(1500);
	       
	        jQuery('#line1').slideDown(2200);
	        jQuery('#line2').slideDown(2200);
	        counter = 2;
	    }
	    else if (counter == 2) {
	        jQuery("#carousel2").fadeOut(1500);
	        jQuery("#carousel3").removeClass(".hidebg").addClass(".showbg");
	        jQuery('#line1').slideUp(100);
	        jQuery('#line2').slideUp(100);
	        jQuery("#carousel3").fadeIn(1500);
	        jQuery('#line1').text("Smart Investment made simple through technology");
	        jQuery('#line2').text('Goal Based,Simple and Diversified  Investment  Solution Tailored to your needs');
	        jQuery('#line1').slideDown(2200);
	        jQuery('#line2').slideDown(2200);
	        counter = 0;
	    }
	    else {
	        jQuery("#carousel3").fadeOut(1500);
	        jQuery("#carousel1").removeClass(".hidebg").addClass(".showbg");
	        jQuery('#line1').slideUp(100);
	        jQuery('#line2').slideUp(100);
	        jQuery("#carousel1").fadeIn(1500);
	        jQuery('#line1').text("Smart Investment made simple through technology");
	        jQuery('#line2').text('Goal Based,Simple and Diversified Investment  Solution Tailored to your needs');
	        jQuery('#line1').slideDown(2200);
	        jQuery('#line2').slideDown(2200);
	        counter = 1;
	    }
	}
	  
	  </script>	
